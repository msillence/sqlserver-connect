<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
    <Appenders>
        <Console name="LogToConsole" target="SYSTEM_OUT">
        	<JsonTemplateLayout EventTemplateUri="classpath:GcpLayout.json"/>	
        </Console>

    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="LogToConsole" />
        </Root>
    </Loggers>
</Configuration>
